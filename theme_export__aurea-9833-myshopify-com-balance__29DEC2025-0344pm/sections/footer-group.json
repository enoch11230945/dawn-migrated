                        "twitter_url": "",
                        "threads_url": "",
                        "linkedin_url": "",
                        "bluesky_url": "",
                        "snapchat_url": "",
                        "pinterest_url": "https://www.pinterest.com/aureajewelryuk",
                        "tumblr_url": "",
                        "vimeo_url": "",
                        "custom_url": ""
                    }
                }
            },
            "block_order": [
                "copyright",
                "policies",
                "social"
            ],
            "settings": {
                "color_scheme": "scheme-2"
            }
        }
    },
    "order": [
        "newsletter_section",
        "footer_utilities"
    ]
