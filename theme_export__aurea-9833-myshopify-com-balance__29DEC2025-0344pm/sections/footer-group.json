{
    "type": "footer",
    "name": "Footer",
    "sections": {
        "newsletter_section": {
            "type": "section",
            "blocks": {
                "newsletter_group": {
                    "type": "group",
                    "name": "Newsletter",
                    "settings": {
                        "content_direction": "row",
                        "vertical_on_mobile": true,
                        "horizontal_alignment": "flex-start",
                        "vertical_alignment": "flex-start",
                        "gap": 24,
                        "width": "fill",
                        "height": "fit",
                        "inherit_color_scheme": true
                    },
                    "blocks": {
                        "newsletter_text_group": {
                            "type": "group",
                            "name": "Text Group",
                            "settings": {
                                "content_direction": "column",
                                "gap": 8,
                                "width": "fill",
                                "height": "fit",
                                "inherit_color_scheme": true
                            },
                            "blocks": {
                                "newsletter_heading": {
                                    "type": "text",
                                    "name": "Heading",
                                    "settings": {
                                        "text": "<p>Stay Connected</p>",
                                        "width": "100%",
                                        "alignment": "left",
                                        "type_preset": "h3",
                                        "font": "var(--font-primary--family)"
                                    }
                                },
                                "newsletter_description": {
                                    "type": "text",
                                    "name": "Description",
                                    "settings": {
                                        "text": "<p>Join our community for exclusive offers and new arrivals.</p>",
                                        "width": "100%",
                                        "alignment": "left",
                                        "type_preset": "rte",
                                        "font": "var(--font-primary--family)"
                                    }
                                }
                            },
                            "block_order": [
                                "newsletter_heading",
                                "newsletter_description"
                            ]
                        },
                        "email_signup": {
                            "type": "email-signup",
                            "name": "Email Signup",
                            "settings": {
                                "width": "custom",
                                "custom_width": 40,
                                "inherit_color_scheme": true,
                                "color_scheme": "scheme-3",
                                "border_style": "underline",
                                "border_width": 1,
                                "border_radius": 0,
                                "input_type_preset": "h4",
                                "style_class": "button-unstyled",
                                "display_type": "text",
                                "label": "Subscribe",
                                "integrated_button": true,
                                "button_type_preset": "h4"
                            }
                        }
                    },
                    "block_order": [
                        "newsletter_text_group",
                        "email_signup"
                    ]
                }
            },
            "block_order": [
                "newsletter_group"
            ],
            "settings": {
                "content_direction": "column",
                "horizontal_alignment": "center",
                "vertical_alignment": "center",
                "gap": 64,
                "section_width": "page-width",
                "color_scheme": "scheme-2",
                "padding-block-start": 64,
                "padding-block-end": 32
            }
        },
        "footer_utilities": {
            "type": "footer-utilities",
            "blocks": {
                "copyright": {
                    "type": "footer-copyright",
                    "settings": {}
                },
                "policies": {
                    "type": "footer-policy-list",
                    "settings": {}
                },
                "social": {
                    "type": "social-links",
                    "settings": {
                        "facebook_url": "",
                        "instagram_url": "https://www.instagram.com/aurea.jewelry.uk",
                        "youtube_url": "",
