{
  "sections": {
    "cart-section": {
      "type": "main-cart",
      "blocks": {
        "cart-page-title": {
          "type": "_cart-title",
          "static": true,
          "settings": {
            "title": "Your Cart",
            "show_count": true,
            "type_preset": "h3",
            "alignment": "left",
            "padding-block-start": 0,
            "padding-block-end": 0
          }
        },
        "cart-page-items": {
          "type": "_cart-products",
          "static": true,
          "settings": {
            "gap": 24,
            "image_ratio": "portrait",
            "dividers": true,
            "vendor": false,
            "padding-block-start": 0,
            "padding-block-end": 0
          }
        },
        "cart-page-summary": {
          "type": "_cart-summary",
          "static": true,
          "settings": {
            "extend_summary": true,
            "inherit_color_scheme": false,
            "color_scheme": "scheme-2",
            "border": "none",
            "border_width": 1,
            "border_radius": 0
          }
        }
      },
      "settings": {
        "section_width": "page-width",
        "color_scheme": "",
        "padding-block-start": 24,
        "padding-block-end": 48
      }
    },
    "trust_bar": {
      "type": "section-trust-bar",
      "blocks": {
        "badge_1": {
          "type": "badge",
          "settings": {
            "icon": "üì¶",
            "text": "Royal Mail Tracked"
          }
        },
        "badge_2": {
          "type": "badge",
          "settings": {
            "icon": "üíß",
            "text": "100% Waterproof"
          }
        },
        "badge_3": {
          "type": "badge",
          "settings": {
            "icon": "‚ôæÔ∏è",
            "text": "Lifetime Warranty"
          }
        },
        "badge_4": {
          "type": "badge",
          "settings": {
            "icon": "üîí",
            "text": "Secure Checkout"
          }
        }
      },
      "block_order": [
        "badge_1",
        "badge_2",
        "badge_3",
        "badge_4"
      ],
      "settings": {}
    },
    "product_recommendations": {
      "type": "product-list",
      "blocks": {
        "static-header": {
          "type": "_product-list-content",
          "name": "Header",
          "static": true,
          "settings": {
            "content_direction": "row",
            "horizontal_alignment": "space-between",
            "vertical_alignment": "flex-end",
            "align_baseline": true,
            "gap": 12,
            "width": "fill"
          },
          "blocks": {
            "header_text": {
              "type": "_product-list-text",
              "name": "Title",
              "settings": {
                "text": "<h3>Complete Your Gift</h3>",
                "width": "fit-content",
                "alignment": "left",
                "type_preset": "h4",
                "font": "var(--font-body--family)"
              }
            }
          },
          "block_order": [
            "header_text"
          ]
        },
        "static-product-card": {
          "type": "_product-card",
          "name": "Product Card",
          "static": true,
          "settings": {
            "product_card_gap": 4,
            "inherit_color_scheme": true
          },
          "blocks": {
            "card_gallery": {
              "type": "_product-card-gallery",
              "name": "Media",
              "settings": {
                "image_ratio": "adapt",
                "border": "none",
                "border_radius": 0
              }
            },
            "card_title": {
              "type": "product-title",
              "name": "Title",
              "settings": {
                "width": "100%",
                "alignment": "left",
                "type_preset": "rte",
                "font": "var(--font-body--family)",
                "color": "var(--color-foreground)"
              }
            },
            "card_price": {
              "type": "price",
              "settings": {
                "show_sale_price_first": true,
                "type_preset": "h6",
                "width": "100%",
                "alignment": "left",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "color": "var(--color-foreground)"
              }
            }
          },
          "block_order": [
            "card_gallery",
            "card_title",
            "card_price"
          ]
        }
      },
      "settings": {
        "collection": "",
        "layout_type": "grid",
        "carousel_on_mobile": false,
        "max_products": 4,
        "columns": 4,
        "mobile_columns": "2",
        "columns_gap": 8,
        "rows_gap": 24,
        "section_width": "page-width",
        "horizontal_alignment": "flex-start",
        "gap": 28,
        "color_scheme": "scheme-1",
        "padding-block-start": 48,
        "padding-block-end": 48
      }
    }
  },
  "order": [
    "cart-section",
    "trust_bar",
    "product_recommendations"
  ]
}